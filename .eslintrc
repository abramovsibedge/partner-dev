{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"env": {
		"browser": true,
		"node": true
	},
	"parser": "babel-eslint",
	"rules": {
		"react/display-name": 1,
		"react/jsx-key": 1,
		"react/jsx-no-duplicate-props": 1,
		"react/jsx-uses-react": 2,
		"react/jsx-uses-vars": 2,
		"react/no-direct-mutation-state": 1,
		"react/react-in-jsx-scope": 2
	},
	"plugins": [
		"react"
	]
}